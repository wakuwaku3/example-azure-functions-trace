{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "None"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Exception;Trace;Event;Request"
      }
    }
  },
  "extensions": {
    "serviceBus": {
      "maxConcurrentCalls": 16,
      "maxMessageBatchSize": 1000
    }
  },
  "concurrency": {
    "dynamicConcurrencyEnabled": true,
    "snapshotPersistenceEnabled": true
  }
}
