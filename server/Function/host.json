{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Warning",
      "Host": "Error",
      "Function": "Error",
      "Host.Aggregator": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Exception;Trace;Event;Request"
      }
    }
  }
}
